<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_color"
    tools:context=".NewActivity">

    <Button
        android:id="@+id/finishBtn"
        android:layout_width="167dp"
        android:layout_height="61dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="Finish"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        android:enabled="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyDefectCataractsFormBtn"
        app:layout_constraintVertical_bias="0.722" />

    <Button
        android:id="@+id/historyPresbyopiaBtn"
        android:layout_width="322dp"
        android:layout_height="61dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="PRESBYOPIA"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyDefectAstimagtismFormBtn" />


    <Button
        android:id="@+id/historyDefectCataractsFormBtn"
        android:layout_width="322dp"
        android:layout_height="61dp"
        android:layout_marginTop="28dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="CATARACTS"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyPresbyopiaBtn" />

    <Button
        android:id="@+id/historyDefectHypermyopiaFormBtn"
        android:layout_width="322dp"
        android:layout_height="61dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="HYPERMYOPIA"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyDefectMyopiaFormBtn" />

    <TextView
        android:id="@+id/historyEyeDefectTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/poppins_semibold"
        android:text="What history of eye defect is in the family?"
        android:textAlignment="center"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyDefectBackBtn" />

    <Button
        android:id="@+id/historyDefectMyopiaFormBtn"
        android:layout_width="322dp"
        android:layout_height="61dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="MYOPIA"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyEyeDefectTextView" />

    <Button
        android:id="@+id/historyDefectAstimagtismFormBtn"
        android:layout_width="322dp"
        android:layout_height="61dp"
        android:layout_marginTop="24dp"
        android:background="@drawable/custom_button"
        android:fontFamily="@font/poppins_extrabold"
        android:text="ASTIGMATISM"
        android:textAllCaps="false"
        android:textColor="@color/h1"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/historyDefectHypermyopiaFormBtn" />

    <ImageView
        android:id="@+id/historyDefectBackBtn"
        android:layout_width="70dp"
        android:layout_height="26dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/back_arrow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>